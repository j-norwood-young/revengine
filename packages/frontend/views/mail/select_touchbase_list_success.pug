extends ../layout

block content
    .container
        .alert.alert-success
            h4.alert-heading Success!
            p.mb-0 You have successfully added #{data.TotalUniqueEmailsSubmitted} members to the Touchbase list "#{ list_name }"
            ul
                li
                    strong Existing Subscribers 
                    = data.reduce((prev, curr) => prev + curr.TotalExistingSubscribers, 0)
                li
                    strong New Subscribers 
                    = data.reduce((prev, curr) => prev + curr.TotalNewSubscribers, 0)